{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "./",
		"module": "commonjs",
		"target": "esnext",
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"strictPropertyInitialization": false,
		"isolatedModules": false,
		"strict": false,
		"noImplicitAny": false,
		"typeRoots" : [
			"../node_modules/@types"
		]
	},
	"exclude": [
		"../node_modules"
	],
	"include": [
		"./*.ts"
	]
}