{
  "name": "Mocha Tests",
  "version": "1.0.0",
  "main": "index.html",
  "type": "module",
  "request": "launch",
  "taget": "es6",
  "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
  "args": [
    "-u",
    "bdd",
    "--timeout",
    "999999",
    "--colors",
    "${workspaceFolder}/test/*.js"
  ],
  "internalConsoleOptions": "openOnSessionStart",
  "scripts": {
    "test": "mocha",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testdeck/mocha": "^0.2.0",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "register-js": "^1.2.2",
    "ts-mocha": "^10.0.0"
  }
}
